{{!< default}}

<h1 style="text-align: center;">{{tag.name}}.</h1>
<ol id="posts-list">
{{#get "posts" order="title asc"}}
  {{#foreach posts}}
  <li>
    <time datetime="{{date format="YYYY-MM-DD"}}">{{date format="YYYY-MM-DD"}}</time>
    <a href="{{url}}">{{title}}</a>
    <span class="post tags">{{tags separator=" "}}</span>
  </li>
  {{/foreach}}
{{/get}}
</ol>
{{pagination}}
