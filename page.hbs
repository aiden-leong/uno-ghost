{{!< default}}

{{#post}}

  <article class="{{post_class}}">
    <header>
      <a title="Tweet '{{title}}'" href=".">
        <h1 class="icon-reverse" id="post-title">{{title}}</h1>
      </a>
    </header>

    <div id="post-content" class="{{post_class}}">
      {{content}}
    </div>

  </article>

{{/post}}
